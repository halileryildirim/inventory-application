<h1>
    <%= title %>
</h1>

<% if (game_list.length) {%>
    <ul>
        <% for(let game of game_list) {%>
            <li>
                <a href="<%=game.url%>">
                    <%=game.name%> | <%=game.platform%>
                </a>
            </li>
            <% } %>
    </ul>
<% } else { %> 
    <p> There are no games.</p>
<% } %>
