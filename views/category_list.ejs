<h1>
    <%= title %>
</h1>

<% if (category_list.length) {%>
    <ul>
        <% for(let category of category_list) {%>
            <li>
                <a href="<%=category.url%>">
                    <%=category.name%>
                </a>
            </li>
            <% } %>
    </ul>
<% } else { %> 
    <p> There are no games.</p>
<% } %>
